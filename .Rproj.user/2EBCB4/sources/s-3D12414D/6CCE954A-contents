setwd("~/OIBioStat/data")

# scripts for final modifications to OIBioStat data

# statins data

a = (statins$VAT == -1)
statins$VAT[a] = NA

summary(statins$VAT)

a = (statins$Smoking == -1)
statins$Smoking[a] = NA
table(statins$Smoking, useNA = "ifany")

a = (statins$Solubility == -1)
statins$Solubility[a] = NA
table(statins$Solubility, useNA = "ifany")
table(statins$Statin, statins$Solubility, useNA = "ifany")

# possible error in the coding for sol.  value 2 shows up



table(statins$GRS)

save(statins, file = statins.rda)
